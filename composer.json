{
    "name": "orchestra/foundation",
    "description": "Orchestra Platform Foundation",
    "homepage": "http://orchestraplatform.com/docs/latest/",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "administrator", "admin", "user-management"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Foundation\\" : "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Tests\\Controller\\" : "tests/Controller/",
            "Orchestra\\Tests\\Feature\\" : "tests/Feature/",
            "Orchestra\\Tests\\Unit\\" : "tests/Unit/"
        }
    },
    "require": {
        "php": ">=7.2",
        "laravel/framework": "~5.9.0",
        "laravie/authen": "^2.1",
        "laravie/html": "~5.9.0",
        "mpociot/reauthenticate": "^1.3",
        "orchestra/asset": "~3.9.0",
        "orchestra/auth": "~3.9.0",
        "orchestra/contracts": "~3.9.0",
        "orchestra/extension": "~3.9.0",
        "orchestra/html": "~3.9.0",
        "orchestra/kernel": "~3.9.0",
        "orchestra/memory": "~3.9.0",
        "orchestra/messages": "~3.9.0",
        "orchestra/model": "~3.9.0",
        "orchestra/notifier": "~3.9.0",
        "orchestra/support": "~3.9.0",
        "orchestra/translation": "~3.9.0",
        "orchestra/view": "~3.9.0",
        "orchestra/widget": "~3.9.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.2",
        "orchestra/installer": "~3.9.0",
        "orchestra/testing": "~3.9.0"
    },
    "suggest": {
        "orchestra/avatar": "Add gravatar support for Orchestra Platform (~3.9).",
        "orchestra/installer": "Allow Orchestra Platform to be installed in new application (~3.9).",
        "orchestra/testing": "Allow better unit testing support for Orchestra Platform (~3.9)."
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit --testsuite=Unit",
            "./vendor/bin/phpunit --testsuite=Controller",
            "./vendor/bin/phpunit --testsuite=Feature"
        ],
        "update:frontend": [
            "bower update",
            "bower-installer",
            "gulp --production"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.9-dev"
        }
    },
    "minimum-stability": "dev"
}
