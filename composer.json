{
    "name": "orchestra/foundation",
    "description": "Orchestra Platform Foundation",
    "homepage": "http://orchestraplatform.com/docs/latest/",
    "keywords": [
        "orchestra-platform",
        "orchestral",
        "laravel",
        "administrator",
        "admin",
        "user-management"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Foundation\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Tests\\Controller\\": "tests/Controller/",
            "Orchestra\\Tests\\Feature\\": "tests/Feature/",
            "Orchestra\\Tests\\Unit\\": "tests/Unit/"
        }
    },
    "require": {
        "php": "^8.0",
        "laravel/framework": "^9.0",
        "laravie/authen": "^3.0",
        "laravie/html": "^7.3",
        "orchestra/asset": "^7.x-dev",
        "orchestra/auth": "7.x-dev",
        "orchestra/contracts": "7.x-dev",
        "orchestra/extension": "7.x-dev",
        "orchestra/html": "7.x-dev",
        "orchestra/kernel": "^7.x.dev",
        "orchestra/memory": "^7.x-dev",
        "orchestra/messages": "7.0.x-dev",
        "orchestra/model": "7.x-dev",
        "orchestra/notifier": "^7.x-dev",
        "orchestra/support": "^7.x-dev",
        "orchestra/translation": "7.x-dev",
        "orchestra/view": "7.0.x-dev",
        "orchestra/widget": "7.x-dev"
    },
    "require-dev": {
        "orchestra/canvas": "^7.0",
        "orchestra/installer": "^7.x-dev",
        "orchestra/testbench": "^7.0",
        "orchestra/testing": "7.x-dev"
    },
    "suggest": {
        "orchestra/avatar": "Add gravatar support for Orchestra Platform (^6.0).",
        "orchestra/installer": "Allow Orchestra Platform to be installed in new application (^6.0).",
        "orchestra/testing": "Allow better unit testing support for Orchestra Platform (^6.0)."
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit --testsuite=Unit --colors=always",
            "./vendor/bin/phpunit --testsuite=Controller --colors=always",
            "./vendor/bin/phpunit --testsuite=Feature --colors=always"
        ],
        "update:frontend": [
            "bower update",
            "bower-installer",
            "gulp --production"
        ]
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "7.0-dev"
        }
    },
    "minimum-stability": "dev",
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/asset"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/auth"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/model"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/dhosa"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/html"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/kernel"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/widget"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/testing"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/installer"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/translation"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/extension"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/contracts"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/memory"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/avatar"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/support"
        },
        {
            "type": "vcs",
            "url": "https://github.com/katsana-external/notifier"
        }
    ]
}
