{
    "name": "orchestra/foundation",
    "description": "Orchestra Platform Foundation",
    "homepage": "http://orchestraplatform.com/docs/latest/",
    "keywords": ["orchestra-platform", "orchestral", "laravel", "administrator", "admin", "user-management"],
    "license": "MIT",
    "authors": [
        {
            "name": "Mior Muhammad Zaki",
            "email": "crynobone@gmail.com",
            "homepage": "https://github.com/crynobone"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orchestra\\Foundation\\" : "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Orchestra\\Tests\\" : "tests/"
        }
    },
    "require": {
        "php": ">=7.1",
        "laravel/framework": "~5.7.0",
        "laravie/authen": "~2.0",
        "laravie/html": "~5.7.0",
        "mpociot/reauthenticate": "~1.2",
        "orchestra/asset": "~3.7.0",
        "orchestra/auth": "~3.7.0",
        "orchestra/contracts": "~3.7.0",
        "orchestra/extension": "~3.7.0",
        "orchestra/html": "~3.7.0",
        "orchestra/kernel": "~3.7.0",
        "orchestra/memory": "~3.7.0",
        "orchestra/messages": "~3.7.0",
        "orchestra/model": "~3.7.0",
        "orchestra/notifier": "~3.7.0",
        "orchestra/support": "~3.7.0",
        "orchestra/translation": "~3.7.0",
        "orchestra/view": "~3.7.0",
        "orchestra/widget": "~3.7.0",
        "predis/predis": "~1.0",
        "symfony/http-foundation": "~4.0.0"
    },
    "require-dev": {
        "orchestra/installer": "~3.7.0",
        "orchestra/testing": "~3.7.0",
        "phpunit/phpunit": "~7.0.1 || ~7.1"
    },
    "suggest": {
        "orchestra/avatar": "Add gravatar support for Orchestra Platform (~3.7).",
        "orchestra/ftp-updater": "Allow Extension with asset to be updated/uploaded environment without SSH support (~3.7).",
        "orchestra/installer": "Allow Orchestra Platform to be installed in new application (~3.7).",
        "orchestra/testing": "Allow better unit testing support for Orchestra Platform (~3.7)."
    },
    "scripts": {
        "test": [
            "./vendor/bin/phpunit --testsuite=Unit",
            "./vendor/bin/phpunit --testsuite=Controller",
            "./vendor/bin/phpunit --testsuite=Feature"
        ],
        "update:frontend": [
            "bower update",
            "bower-installer",
            "gulp"
        ]
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.7-dev"
        }
    },
    "minimum-stability": "dev"
}
